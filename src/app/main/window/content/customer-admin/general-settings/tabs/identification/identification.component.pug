app-manage-list(
  (langChange)="load($event)",
  (new)="addItem(+$event)",
  (save)="save(+$event)",
  [disabled]="isLoading | async"
)
app-loading.loading-bar(*ngIf="isLoading | async")
br
.list.row
  .item.form-group(*ngFor="let doctype of documentTypes")
    label {{doctype.dt_name}}
    .form-group
      input([(ngModel)]="doctype.dtl_value")
      i.mdi.mdi-close((click)="deleteItem(doctype)")
