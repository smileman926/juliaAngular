    "@angular/core": "~8.2.13",
    "@angular/forms": "~8.2.13",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.13",
    "@angular/platform-browser-dynamic": "~8.2.13",
    "@angular/router": "~8.2.13",
    "@ng-bootstrap/ng-bootstrap": "^5.1.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@rxweb/reactive-form-validators": "^1.9.1",
    "@wishtack/reactive-component-loader": "^1.0.0",
    "angular-star-rating": "^4.0.0-beta.3",
    "angular2-draggable": "^2.3.1",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.9.4",
    "css-star-rating": "^1.2.4",
    "date-fns": "^1.30.1",
    "dayjs": "^1.8.14",
    "deepmerge": "^4.2.2",
    "delay": "^4.3.0",
    "file-saver": "^2.0.2",
