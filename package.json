{
  "name": "julia",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:local": "ng serve -c local",
    "start:localssl": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --disableHostCheck --configuration=localssl --liveReload=true --poll 3000 --watch=true --ssl --ssl-key ssl/localhost.key --ssl-cert ssl/localhost.crt",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:staging": "ng build --configuration=staging",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node ./ng-add-pug-loader.js",
    "deploy:staging": "env-cmd bash -c 'sudo scp -i $SSH_KEY -r ./dist/julia/* $SSH_USER@$SSH_HOST:$DEPLOY_TARGET'",
    "predeploy:staging": "ng build --configuration=production --optimization=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.13",
